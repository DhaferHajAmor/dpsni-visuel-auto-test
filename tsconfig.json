{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom"],
    "types": ["cypress"],
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@fixtures/*": ["src/__tests__/partials/*"],
      "@mocks/*": ["src/__tests__/mocks/*"]
    }
  },
  "include": ["src/*.ts", "vitest.config.ts"]
}
